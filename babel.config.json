{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current"
				}
			}
		],
		"@babel/preset-react"
	],
	"plugins": ["babel-plugin-styled-components"],
	"env": {
		"development": {
			"plugins": ["transform-es2015-modules-commonjs"]
		},
		"test": {
			"plugins": ["transform-es2015-modules-commonjs"],
			"presets": ["@babel/preset-react"]
		}
	}
}
